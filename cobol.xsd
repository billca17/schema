          <xsd:element name="Returning" type="OperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="SetMethod" type="GetSetMethodType" maxOccurs="unbounded" minOccurs="0"/>
          <xsd:element name="GetMethod" type="GetSetMethodType" maxOccurs="unbounded" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute name="InlineSignature" type="xsd:boolean" default="false"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="TableInitializerOperandType">
    <xsd:annotation>
      <xsd:documentation>Representation of a Table Initializer Operand</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="OperandType">
        <xsd:sequence>
          <xsd:element name="TableInitializer" type="TableInitializerType" maxOccurs="1" minOccurs="0" /> 
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="TableInitializerType">
    <xsd:annotation>
      <xsd:documentation>Representation of a sequence of Elements for Table Initializer</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="OperandType">
        <xsd:sequence>
          <xsd:element name="SubInfo" type="SubInfoType" maxOccurs="unbounded" minOccurs="0"/>
          <xsd:element name="Elements" type="OperandType" maxOccurs="unbounded" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute name="TypeName" type="xsd:string"/>
        <xsd:attribute name="Dimensions" type="xsd:integer"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="PropertyOperandType">
    <xsd:annotation>
      <xsd:documentation>Representation of a property operand</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="OperandType">
        <xsd:sequence>
          <xsd:element name="Object" type="OperandType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute name="Name" type="xsd:string"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="QueryOperandType">
    <xsd:annotation>
      <xsd:documentation>Representation of a set indexer to Query operand</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="OperandType">
        <xsd:sequence>
          <xsd:element name="Select"  type="QuerySelectType" maxOccurs="1" minOccurs="1"/>
          <xsd:element name="From" type="OperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="Joined" type="QueryJoinedType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="Where" type="OperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="Returning" type="OperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="OrderBy" type="QueryOrderByType" maxOccurs="16" minOccurs="0"/>
          <xsd:element name="GroupBy" type="OperandType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="QueryOrderByType">
    <xsd:annotation>
      <xsd:documentation>List of Order By clauses</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="OperandType">
        <xsd:sequence>
          <xsd:element name="Order" type="OperandType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute name="Descending" type="xsd:boolean" default="false"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="QuerySelectType">
    <xsd:annotation>
      <xsd:documentation>Representation of a Query Select item</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="CobolObjectType">
        <xsd:sequence/>
        <xsd:attribute name="Name" type="xsd:string"/>
        <xsd:attribute name="CID" type="xsd:ID"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="QueryJoinedType">
    <xsd:annotation>
      <xsd:documentation>Representation of a Query Select item</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="CobolObjectType">
        <xsd:sequence>
          <xsd:element name="From" type="OperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="Left" type="OperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="Right" type="OperandType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute name="Name" type="xsd:string"/>
        <xsd:attribute name="CID" type="xsd:ID"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="RaiseStatementType">
    <xsd:annotation>
      <xsd:documentation>Representation of a Procedure Division RAISE statement</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="StatementType">
        <xsd:sequence>
          <xsd:element name="Exception" type="OperandType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="ReadCollectionStatementType">
    <xsd:annotation>
      <xsd:documentation>Representation of a Procedure Division READ Collection statement</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="StatementType">
        <xsd:sequence>
          <xsd:element name="Collection" type="OperandType" maxOccurs="1" minOccurs="1"/>
          <xsd:element name="Into" type="OperandType" maxOccurs="1" minOccurs="1"/>
          <xsd:element name="Key" type="OperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="InvalidKey" type="StatementBlockType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="NotInvalidKey" type="StatementBlockType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="ReadKeyType">
    <xsd:annotation>
      <xsd:documentation>Representation of a Procedure Division Read Key phrase</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ClauseType">
        <xsd:sequence>
          <xsd:element name="KeyOperand" type="OperandType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute name="KeyID" type="xsd:integer"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="ReadStatementType">
    <xsd:annotation>
      <xsd:documentation>Representation of a Procedure Division READ statement</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="StatementType">
        <xsd:sequence>
          <xsd:element name="File" type="FileOperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="Into" type="OperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="Key" type="ReadKeyType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="InvalidKey" type="StatementBlockType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="NotInvalidKey" type="StatementBlockType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="AtEnd" type="StatementBlockType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="NotAtEnd" type="StatementBlockType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute name="LockMode" type="LockModeEnumType"/>
        <xsd:attribute name="ReadType" type="ReadTypeEnumType"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:simpleType name="ReadTypeEnumType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="NEXT"/>
      <xsd:enumeration value="PREVIOUS"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="ReceiveThreadStatementType">
    <xsd:annotation>
      <xsd:documentation>Representation of a Procedure Division RECEIVE THREAD statement (ACU)</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="StatementType">
        <xsd:sequence>
          <xsd:element name="Message" type="OperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="FromThread" type="OperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="Size" type="OperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="Status" type="OperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="Timeout" type="OperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="ThreadIn" type="OperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="OnException" type="StatementBlockType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="NotOnException" type="StatementBlockType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute name="ReceiveType" type="ReceiveWaitTypeEnumType"/>
        <xsd:attribute name="NoWait" type="xsd:boolean" default="false"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:simpleType name="ReceiveWaitTypeEnumType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="AnyThread"/>
      <xsd:enumeration value="Thread"/>
      <xsd:enumeration value="LastThread"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="BlockContainsEnumType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="RECORDS"/>
      <xsd:enumeration value="CHARACTERS"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="RecordingModeEnumType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="F"/>
      <xsd:enumeration value="V"/>
      <xsd:enumeration value="U"/>
      <xsd:enumeration value="S"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="RecordPhraseEnumType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="CONTAINS"/>
      <xsd:enumeration value="CONTAINSRANGE"/>
      <xsd:enumeration value="VARYING"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="RecordPositionOperandType">
    <xsd:annotation>
      <xsd:documentation>Representation of a RECORD-POSITION OF operand</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="OperandType">
        <xsd:sequence>
          <xsd:element name="Operand" type="OperandType" maxOccurs="1" minOccurs="1"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="RecordSequentialFileType">
    <xsd:annotation>
      <xsd:documentation>Representation of a COBOL Record Sequential File</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="CobolFileType">
        <xsd:sequence>
          <xsd:element name="Reserve" type="CobolFileReserveType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="Padding" type="CobolFilePaddingType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="ReferenceModifiedOperandType">
    <xsd:annotation>
      <xsd:documentation>Representation of a reference modified operand</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="OperandType">
        <xsd:sequence>
          <xsd:element name="ModifiedOperand" type="OperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="StartPosition" type="OperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="Length" type="OperandType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute name="Basis" type="xsd:int" default="1"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="RelationConditionType">
    <xsd:annotation>
      <xsd:documentation>Representation of a simple condition</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ConditionType">
        <xsd:sequence>
          <xsd:element name="Left" type="OperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="Right" type="OperandType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute name="Relation" type="RelationEnumType"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:simpleType name="RelationEnumType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="EQUAL"/>
      <xsd:enumeration value="EXCEEDS"/>
      <xsd:enumeration value="GREATER"/>
      <xsd:enumeration value="GREATEROREQUAL"/>
      <xsd:enumeration value="LESS"/>
      <xsd:enumeration value="LESSOREQUAL"/>
      <xsd:enumeration value="UNEQUAL"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="RelativeFileType">
    <xsd:annotation>
      <xsd:documentation>Representation of a COBOL Relative File</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="CobolFileType">
        <xsd:sequence>
          <xsd:element name="Reserve" type="CobolFileReserveType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="RelativeKey" type="DataOperandType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="ReleaseStatementType">
    <xsd:annotation>
      <xsd:documentation>Representation of a Procedure Division RELEASE statement</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="StatementType">
        <xsd:sequence>
          <xsd:element name="File" type="FileOperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="Record" type="OperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="From" type="OperandType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="ReplacedBlockType">
    <xsd:annotation>
      <xsd:documentation>Representation of a COBOL Replaced Text Block (COPY REPLACING or REPLACE)</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="CobolObjectType">
        <xsd:sequence>
          <xsd:element name="ReplacedLines" type="ReplacedLineType" maxOccurs="unbounded" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute name="FirstLine" type="xsd:integer"/>
        <xsd:attribute name="LastLine" type="xsd:integer"/>
        <xsd:attribute name="OriginalLines" type="xsd:integer"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="ReplacedLineType">
    <xsd:annotation>
      <xsd:documentation>Representation of a COBOL Replaced Text Line (COPY REPLACING or REPLACE)</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="CobolObjectType">
        <xsd:sequence/>
        <xsd:attribute name="Indentation" type="xsd:integer"/>
        <xsd:attribute name="Text" type="xsd:string"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="SuppressCategoryOperandType">
    <xsd:annotation>
      <xsd:documentation>Representation of a category in a JSON/XML GENERATE .. SUPPRESS phrase</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="OperandType">
        <xsd:sequence/>
        <xsd:attribute name="Category" type="SuppressCategoryEnumType"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="ReplacingCategoryOperandType">
    <xsd:annotation>
      <xsd:documentation>Representation of a category in an INITIALIZE statement REPLACING phrase</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="OperandType">
        <xsd:sequence/>
        <xsd:attribute name="Category" type="InitializeCategoryEnumType"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="ReplacingClauseType">
    <xsd:annotation>
      <xsd:documentation>Representation of an INITIALIZE statement REPLACING phrase</xsd:documentation>
      <xsd:documentation>Note:  Multiple categories are represented by repeated instances of this type</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ClauseType">
        <xsd:sequence>
          <xsd:element name="Category" type="ReplacingCategoryOperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="Value" type="OperandType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="RepositoryType">
    <xsd:annotation>
      <xsd:documentation>Representation of a COBOL Repository paragraph</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="CobolObjectType">
        <xsd:sequence>
          <xsd:element name="Entries" type="RepositoryEntryType" maxOccurs="unbounded" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="RepositoryEntryType">
    <xsd:annotation>
      <xsd:documentation>Representation of a COBOL Repository entry</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="CobolObjectType">
        <xsd:sequence/>
        <xsd:attribute name="CID" type="xsd:ID"/>
        <xsd:attribute name="Type" type="RepositoryTypeEnumType"/>
        <xsd:attribute name="Name" type="xsd:string"/>
        <xsd:attribute name="ExternalName" type="xsd:string"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:simpleType name="RepositoryTypeEnumType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="CLASS"/>
      <xsd:enumeration value="INTERFACE"/>
      <xsd:enumeration value="PROGRAM"/>
      <xsd:enumeration value="FUNCTION"/>
      <xsd:enumeration value="DELEGATE"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="ResetCollectionStatementType">
    <xsd:annotation>
      <xsd:documentation>Representation of a Procedure Division RESET Collection statement</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="StatementType">
        <xsd:sequence>
          <xsd:element name="Collection" type="OperandType" maxOccurs="1" minOccurs="1"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="ReservedSectionType" abstract="true">
    <xsd:annotation>
      <xsd:documentation>Representation of a COBOL Section:
        When the Line and Column attributes are missing,
        the section header is missing in the source but
        there is content that requires the header
        (MF Visual COBOL allows this).  The EndLine and EndColumn
        attributes indicate where this content ended.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="CobolObjectType">
        <xsd:sequence/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="ReturnStatementType">
    <xsd:annotation>
      <xsd:documentation>Representation of a Procedure Division RETURN statement</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="StatementType">
        <xsd:sequence>
          <xsd:element name="File" type="FileOperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="Into" type="OperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="AtEnd" type="StatementBlockType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="NotAtEnd" type="StatementBlockType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="RewriteCollectionStatementType">
    <xsd:annotation>
      <xsd:documentation>Representation of a Procedure Division REWRITE Collection statement</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="StatementType">
        <xsd:sequence>
          <xsd:element name="Collection" type="OperandType" maxOccurs="1" minOccurs="1"/>
          <xsd:element name="From" type="OperandType" maxOccurs="1" minOccurs="1"/>
          <xsd:element name="Key" type="OperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="InvalidKey" type="StatementBlockType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="NotInvalidKey" type="StatementBlockType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="RewriteStatementType">
    <xsd:annotation>
      <xsd:documentation>Representation of a Procedure Division REWRITE statement</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="StatementType">
        <xsd:sequence>
          <xsd:element name="File" type="FileOperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="Record" type="OperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="From" type="OperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="InvalidKey" type="StatementBlockType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="NotInvalidKey" type="StatementBlockType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="RollbackStatementType">
    <xsd:annotation>
      <xsd:documentation>Representation of a Procedure Division ROLLBACK statement</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="StatementType">
        <xsd:sequence/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="SameRecordAreaType">
    <xsd:annotation>
      <xsd:documentation>Representation of a COBOL SAME RECORD AREA clause</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="CobolObjectType">
        <xsd:sequence>
          <xsd:element name="Files" type="FileOperandType" maxOccurs="unbounded" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="ScreenDataItemType">
    <xsd:annotation>
      <xsd:documentation>Representation of an ACU Screen declaration (non-control) data item</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ScreenGenericItemType"/>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="ScreenGenericItemType" abstract="true">
    <!-- Added for ACU -->
    <xsd:annotation>
      <xsd:documentation>Representation of a generic (control or non-control) ACU screen section data item</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="DataItemType">
        <xsd:sequence>
          <xsd:element name="AfterStart" type="ProcedureReferenceType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="AfterEnd" type="ProcedureReferenceType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="BeforeStart" type="ProcedureReferenceType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="BeforeEnd" type="ProcedureReferenceType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="EventStart" type="ProcedureReferenceType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="EventEnd" type="ProcedureReferenceType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="ExceptionStart" type="ProcedureReferenceType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="ExceptionEnd" type="ProcedureReferenceType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="Background-color" type="OperandType" minOccurs="0" maxOccurs="1"/>
          <xsd:element name="Color" type="OperandType" minOccurs="0" maxOccurs="1"/>
          <xsd:element name="Control" type="OperandType" minOccurs="0" maxOccurs="1"/>
          <xsd:element name="Enabled" type="OperandType" minOccurs="0" maxOccurs="1"/>
          <xsd:element name="Font" type="OperandType" minOccurs="0" maxOccurs="1"/>
          <xsd:element name="Foreground-color" type="OperandType" minOccurs="0" maxOccurs="1"/>
          <xsd:element name="Help-id" type="OperandType" minOccurs="0" maxOccurs="1"/>
          <xsd:element name="Identification" type="OperandType" minOccurs="0" maxOccurs="1"/>
          <xsd:element name="Key" type="OperandType" minOccurs="0" maxOccurs="1"/>
          <xsd:choice>
            <!-- Lines -->
            <xsd:element name="Lines" type="OperandType" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="LinesCells" type="OperandType" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="LinesPixels" type="OperandType" minOccurs="0" maxOccurs="1"/>
          </xsd:choice>
          <xsd:element name="Max-height" type="OperandType" minOccurs="0" maxOccurs="1"/>
          <xsd:element name="Max-width" type="OperandType" minOccurs="0" maxOccurs="1"/>
          <xsd:element name="Menu" type="OperandType" minOccurs="0" maxOccurs="1"/>
