      <xsd:extension base="StatementType">
        <xsd:sequence>
          <xsd:element name="DataOperand" type="OperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="File" type="FileOperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="Keys" type="SortKeyType" maxOccurs="unbounded" minOccurs="0"/>
          <xsd:element name="InputStart" type="ProcedureReferenceType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="InputEnd" type="ProcedureReferenceType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="Using" type="FileOperandType" maxOccurs="unbounded" minOccurs="0"/>
          <xsd:element name="OutputStart" type="ProcedureReferenceType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="OutputEnd" type="ProcedureReferenceType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="Giving" type="FileOperandType" maxOccurs="unbounded" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute name="Duplicates" type="xsd:boolean" default="false"/>
        <xsd:attribute name="TableSort" type="xsd:boolean" default="false"/>
        <xsd:attribute name="KeyArea" type="xsd:boolean" default="false"/>
        <xsd:attribute name="CollatingSequence" type="xsd:string"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="SourceComputerType">
    <xsd:annotation>
      <xsd:documentation>Representation of a COBOL Source-Computer paragraph</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="CobolObjectType">
        <xsd:sequence>
          <xsd:element name="DebuggingMode" type="DebuggingModeType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="SourceFileType">
    <xsd:annotation>
      <xsd:documentation>Representation of a COBOL Main or Copybook</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="CobolObjectType">
        <xsd:sequence>
          <xsd:element name="SourceFormat" type="SourceFormatType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="FileStats" type="FileStatsType" maxOccurs="1" minOccurs="0"/>
          <xsd:sequence maxOccurs="unbounded">
             <xsd:element name="ReplacedBlocks" type="ReplacedBlockType" maxOccurs="unbounded" minOccurs="0"/>
             <xsd:element name="Copybooks" type="SourceFileType" maxOccurs="unbounded" minOccurs="0"/>
          </xsd:sequence>
        </xsd:sequence>
        <xsd:attribute name="FileSize" type="xsd:long"/>
        <xsd:attribute name="LastWrite" type="xsd:dateTime"/>
        <xsd:attribute name="ReplacementPath" type="xsd:string"/>
        <xsd:attribute name="CopyLine" type="xsd:int"/>
        <xsd:attribute name="LastLine" type="xsd:int"/>
        <xsd:attribute name="FirstLine" type="xsd:int"/>
        <xsd:attribute name="PathIsRelative" type="xsd:boolean" default="false">
          <xsd:annotation>
            <xsd:documentation>
              If true the Path of the SourceStructure is relative to the the project directory.
              If false the Path is absolute.
            </xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="Path" type="xsd:string"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:simpleType name="SourceFormatEnumType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Fixed"/>
      <xsd:enumeration value="Free"/>
      <xsd:enumeration value="Variable"/>
      <xsd:enumeration value="Terminal"/>
      <xsd:enumeration value="NotSet"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="SourceFormatType">
    <xsd:annotation>
      <xsd:documentation>Representation of a COBOL SourceFormat Directive</xsd:documentation>
      <xsd:documentation>  (last setting of sourceformat directive; possibly not useful)</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="CobolObjectType">
        <xsd:sequence/>
        <xsd:attribute name="Value" type="SourceFormatEnumType"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="SourceMappingType">
    <xsd:annotation>
      <xsd:documentation>Representation of a Source Mapping in an Embedded Fragment of an EXEC statement</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="CobolObjectType">
        <xsd:sequence/>
        <xsd:attribute name="CharPosition" type="xsd:integer"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="SpecialNamesType">
    <xsd:annotation>
      <xsd:documentation>Representation of a COBOL Special-Names paragraph</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="CobolObjectType">
        <xsd:sequence>
          <xsd:choice maxOccurs="unbounded" minOccurs="0">
            <xsd:element name="Switches" type="SwitchType"/>
            <xsd:element name="Alphabets" type="AlphabetType"/>
            <xsd:element name="SymbolicCharacters" type="SymbolicCharacterType"/>
            <xsd:element name="Classes" type="ClassNameType"/>
            <xsd:element name="Currency" type="CurrencyType"/>
            <xsd:element name="Decimal-point" type="DecimalPointType"/>
            <xsd:element name="Console" type="ConsoleType"/>
            <xsd:element name="CallConventions" type="CallConventionType"/>
            <xsd:element name="Cursor" type="DataOperandType"/>
            <xsd:element name="CrtStatus" type="DataOperandType"/>
            <xsd:element name="ChartStatus" type="DataOperandType"/>
            <xsd:element name="EventStatus" type="DataOperandType"/>
            <xsd:element name="ScreenControl" type="DataOperandType"/>
            <xsd:element name="NumericSign" type="NumericSignType"/>
          </xsd:choice>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="SpecialRegistersType">
    <xsd:annotation>
      <xsd:documentation>Representation of COBOL Special-Register Items</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ReservedSectionType">
        <xsd:sequence>
          <xsd:element name="DataItems" type="DataItemType" maxOccurs="unbounded" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="StartKeyType">
    <xsd:annotation>
      <xsd:documentation>Representation of a Procedure Division Start Key phrase</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ClauseType">
        <xsd:sequence>
          <xsd:element name="KeyOperand" type="OperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="KeyLength" type="OperandType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute name="KeyID" type="xsd:integer"/>
        <xsd:attribute name="Relation" type="RelationEnumType"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="StartStatementType">
    <xsd:annotation>
      <xsd:documentation>Representation of a Procedure Division START statement</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="StatementType">
        <xsd:sequence>
          <xsd:element name="File" type="FileOperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="Key" type="StartKeyType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="InvalidKey" type="StatementBlockType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="NotInvalidKey" type="StatementBlockType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="StartTransactionStatementType">
    <xsd:annotation>
      <xsd:documentation>Representation of a Procedure Division START TRANSACTION statement</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="StatementType">
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="StatementBlockType">
    <xsd:annotation>
      <xsd:documentation>Representation of a Procedure Division statement block</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="CobolObjectType">
        <xsd:sequence>
          <xsd:element name="Statements" type="StatementType" maxOccurs="unbounded" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="DataDivisionExecBlockType">
    <xsd:annotation>
      <xsd:documentation>Representation of a Data Division Exec statement block</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="CobolObjectType">
        <xsd:sequence>
          <xsd:element name="Statements" type="ExecStatementType" maxOccurs="unbounded" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="StatementType" abstract="true">
    <xsd:annotation>
      <xsd:documentation>Representation of a Procedure Division statement</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="CobolObjectType">
        <xsd:sequence/>
        <xsd:attribute name="Continuation" type="xsd:boolean" default="false"/>
        <xsd:attribute name="Continued" type="xsd:boolean" default="false"/>
        <xsd:attribute name="HasChild" type="xsd:boolean" default="false"/>
        <xsd:attribute name="HasGoback" type="xsd:boolean" default="false"/>
        <xsd:attribute name="HasNextSentence" type="xsd:boolean" default="false"/>
        <xsd:attribute name="HasTransfer" type="xsd:boolean" default="false"/>
        <xsd:attribute name="LocalScope" type="xsd:boolean" default="false"/>
        <xsd:attribute name="PeriodFollows" type="xsd:boolean" default="false"/>
        <xsd:attribute name="ExplicitScope" type="xsd:boolean" default="false"/>
        <xsd:attribute name="PLine" type="xsd:int" default="0"/>
        <xsd:attribute name="PColumn" type="xsd:int" default="0"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="StopIteratorStatementType">
    <xsd:annotation>
      <xsd:documentation>Representation of a Procedure Division STOP ITERATOR statement</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="StatementType">
        <xsd:sequence/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="StopLiteralStatementType">
    <xsd:annotation>
      <xsd:documentation>Representation of a Procedure Division STOP literal statement</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="StatementType">
        <xsd:sequence>
          <xsd:element name="Value" type="OperandType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="StopRunStatementType">
    <xsd:annotation>
      <xsd:documentation>Representation of a Procedure Division STOP RUN statement</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="StatementType">
        <xsd:sequence>
          <xsd:element name="Returning" type="OperandType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="StopThreadStatementType">
    <xsd:annotation>
      <xsd:documentation>Representation of a Procedure Division STOP THREAD statement</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="StatementType">
        <xsd:sequence>
          <xsd:element name="ThreadID" type="DataOperandType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="StringStatementType">
    <xsd:annotation>
      <xsd:documentation>Representation of a Procedure Division STRING statement</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="StatementType">
        <xsd:sequence>
          <xsd:element name="Operands" type="StringStatementClauseType" maxOccurs="unbounded" minOccurs="0"/>
          <xsd:element name="Target" type="OperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="Pointer" type="OperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="OnOverflow" type="StatementBlockType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="NotOnOverflow" type="StatementBlockType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="StringStatementClauseType">
    <xsd:annotation>
      <xsd:documentation>Representation of a phrase within a Procedure Division STRING statement</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ClauseType">
        <xsd:sequence>
          <xsd:element name="Operand" type="OperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="Delimiter" type="OperandType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute name="SizeDelimiter" type="xsd:boolean" default="false"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="SubInfoType">
    <xsd:annotation>
      <xsd:documentation>Representation of a COBOL Occurs item</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="CobolObjectType">
        <xsd:sequence>
          <xsd:element name="Depending" type="OperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="CountIn" type="OperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:choice maxOccurs="unbounded" minOccurs="0">
            <xsd:element name="Keys" type="OccursKeyType" maxOccurs="unbounded" minOccurs="0"/>
            <xsd:element name="Indexes" type="IndexNameListType" maxOccurs="1" minOccurs="0"/>
          </xsd:choice>
        </xsd:sequence>
        <xsd:attribute name="Max" type="xsd:integer"/>
        <xsd:attribute name="Min" type="xsd:integer"/>
        <xsd:attribute name="Jagged" type="xsd:boolean" default="false"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="SubscriptedOperandType">
    <xsd:annotation>
      <xsd:documentation>Representation of a subscripted operand</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="OperandType">
        <xsd:sequence>
          <xsd:element name="Table" type="DataOperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="Subscripts" type="OperandType" maxOccurs="unbounded" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute name="Basis" type="xsd:int" default="1"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="SubtractStatementType">
    <xsd:annotation>
      <xsd:documentation>Representation of a Procedure Division SUBTRACT statement</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="StatementType">
        <xsd:sequence>
          <xsd:element name="Sources" type="OperandType" maxOccurs="unbounded" minOccurs="0"/>
          <xsd:element name="Targets" type="OperandType" maxOccurs="unbounded" minOccurs="0"/>
          <xsd:element name="OnSizeError" type="StatementBlockType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="NotOnSizeError" type="StatementBlockType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute name="Corresponding" type="xsd:boolean" default="false"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="SwitchType">
    <xsd:annotation>
      <xsd:documentation>Representation of a COBOL Switch in Special-Names</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="CobolItemType">
        <xsd:all>
          <xsd:element name="SwitchOn" type="SwitchConditionType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="SwitchOff" type="SwitchConditionType" maxOccurs="1" minOccurs="0"/>
        </xsd:all>
        <xsd:attribute name="Value" type="xsd:integer"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="SwitchConditionType">
    <xsd:annotation>
      <xsd:documentation>Representation of a COBOL Switch condition</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="CobolItemType">
        <xsd:sequence/>
        <xsd:attribute name="On" type="xsd:boolean" default="false"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="SymbolicCharacterType">
    <xsd:annotation>
      <xsd:documentation>Representation of an SYMBOLIC CHARACTERS clause in the special names paragraph</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="CobolItemType">
        <xsd:sequence/>
        <xsd:attribute name="Value" type="xsd:integer"/>  <!-- 0-relative and translated by alphabet, if one -->
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="SyncStatementType">
    <xsd:annotation>
      <xsd:documentation>Representation of a Procedure Division Sync block statement</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="StatementType">
        <xsd:sequence>
          <xsd:element name="Operand" type="OperandType" maxOccurs="unbounded" minOccurs="0"/>
          <xsd:element name="Statements" type="StatementType" maxOccurs="unbounded" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="ThreadLocalStorageSectionType">
    <xsd:annotation>
      <xsd:documentation>Representation of a COBOL Thread-Local-Storage section</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ReservedSectionType">
        <xsd:sequence>
          <xsd:element name="DataItems" type="DataItemType" maxOccurs="unbounded" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="ThreadPointerDataItemType">
    <xsd:annotation>
      <xsd:documentation>Representation of a COBOL Thread-Pointer data item</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="DataItemType">
        <xsd:sequence/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="ThruValueType">
    <xsd:annotation>
      <xsd:documentation>Representation of a COBOL Level 88 Thru phrase</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="CobolObjectType">
        <xsd:sequence>
          <xsd:element name="Lower" type="OperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="Upper" type="OperandType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="TimeOfDayOperandType">
    <xsd:annotation>
      <xsd:documentation>Representation of a TIME-OF-DAY operand</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="OperandType">
        <xsd:sequence/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="ToDefaultClauseType">
    <xsd:annotation>
      <xsd:documentation>Representation of a TO DEFAULT phrase within a Procedure Division statement</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ClauseType">
        <xsd:sequence/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="ToValueType">
    <xsd:annotation>
      <xsd:documentation>Representation of a To value</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="CobolObjectType">
        <xsd:sequence/>
        <xsd:attribute name="Value" type="xsd:integer"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="ToValueClauseType">
    <xsd:annotation>
      <xsd:documentation>Representation of a TO VALUE phrase within a Procedure Division INITIALIZE statement</xsd:documentation>
      <xsd:documentation>Note:  Multiple categories are represented by repeated Category elements</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ClauseType">
        <xsd:sequence>
          <xsd:element name="Category" type="ReplacingCategoryOperandType" maxOccurs="20" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="TransformClauseType">
    <xsd:annotation>
      <xsd:documentation>Representation of a phrase within a Procedure Division TRANSFORM statement</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="InspectClauseType">
        <xsd:sequence/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="TransformStatementType">
    <xsd:annotation>
      <xsd:documentation>Representation of a Procedure Division TRANSFORM statement</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="StatementType">
        <xsd:sequence>
          <xsd:element name="Operands" type="OperandType" maxOccurs="unbounded" minOccurs="0"/>
          <xsd:element name="TransformClauses" type="TransformClauseType" maxOccurs="unbounded" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:simpleType name="TrueFalseEnumType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="SETTRUE"/>
      <xsd:enumeration value="SETFALSE"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="TrueOperandType">
    <xsd:annotation>
      <xsd:documentation>Representation of operand TRUE</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="OperandType">
        <xsd:sequence/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="TryStatementType">
    <xsd:annotation>
      <xsd:documentation>Representation of a Procedure Division Try/Catch block</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="StatementType">
        <xsd:sequence>
          <xsd:element name="TryBlock" type="StatementBlockType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="Catches" type="CatchBlockType" maxOccurs="unbounded" minOccurs="0"/>
          <xsd:element name="FinallyBlock" type="StatementBlockType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="TypeNameType">
    <xsd:annotation>
      <xsd:documentation>Representation of a type name</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="CobolObjectType">
        <xsd:sequence/>
        <xsd:attribute name="TypeName" type="xsd:string"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="TypeOfOperandType">
    <xsd:annotation>
      <xsd:documentation>Representation of a TYPE OF operand</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="OperandType">
        <xsd:sequence>
          <xsd:element name="Operand" type="OperandType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="UnaryExpressionType">
    <xsd:annotation>
      <xsd:documentation>Representation of a unary expression</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="OperandType">
        <xsd:sequence>
          <xsd:element name="Left" type="OperandType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute name="Operator" type="UnaryExpressionEnumType"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:simpleType name="UnaryExpressionEnumType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="PLUS"/>
      <xsd:enumeration value="MINUS"/>
      <xsd:enumeration value="NOT"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="UnlockStatementAbstractType" abstract="true">
    <xsd:annotation>
      <xsd:documentation>Representation of an abstract Procedure Division UNLOCK statement</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="StatementType">
        <xsd:sequence/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="UnlockAllStatementType">
    <xsd:annotation>
      <xsd:documentation>Representation of a Procedure Division UNLOCK ALL statement</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="UnlockStatementAbstractType">
        <xsd:sequence/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="UnlockStatementType">
    <xsd:annotation>
      <xsd:documentation>Representation of a Procedure Division UNLOCK statement</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="UnlockStatementAbstractType">
        <xsd:sequence>
          <xsd:element name="File" type="FileOperandType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="UnlockThreadStatementType">
    <xsd:annotation>
      <xsd:documentation>Representation of a Procedure Division UNLOCK THREAD statement</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="UnlockStatementAbstractType">
        <xsd:sequence/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="UnstringStatementType">
    <xsd:annotation>
      <xsd:documentation>Representation of a Procedure Division UNSTRING statement</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="StatementType">
        <xsd:sequence>
          <xsd:element name="Source" type="OperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:choice maxOccurs="unbounded" minOccurs="0">
              <xsd:element name="DelimitedBy" type="OperandType" />
              <xsd:element name="DelimitedByAll" type="OperandType" />
          </xsd:choice>
          <xsd:sequence minOccurs="1" maxOccurs="unbounded">
            <xsd:element name="Targets" type="OperandType" maxOccurs="1" minOccurs="0"/>
            <xsd:element name="Count" type="OperandType" maxOccurs="1" minOccurs="0"/>
            <xsd:element name="Delimiter" type="OperandType" maxOccurs="1" minOccurs="0"/>
          </xsd:sequence>
          <xsd:element name="Pointer" type="OperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="Tallying" type="OperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="OnOverflow" type="StatementBlockType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="NotOnOverflow" type="StatementBlockType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:simpleType name="UpDownEnumType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="UP"/>
      <xsd:enumeration value="DOWN"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="UseDebugTypeEnumType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="AllProcedures"/>
      <xsd:enumeration value="AllRefsIdentifier"/>
      <xsd:enumeration value="CDName"/>
      <xsd:enumeration value="FileName"/>
      <xsd:enumeration value="Identifier"/>
      <xsd:enumeration value="Procedure"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="UseDebugType">
    <xsd:annotation>
      <xsd:documentation>Representation of a specification phrase within a Procedure Division USE FOR DEBUGGING statement</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ClauseType">
        <xsd:sequence>
          <xsd:element name="CD" type="CDOperandType" maxOccurs="unbounded" minOccurs="0"/>
          <xsd:element name="Identifier" type="OperandType" maxOccurs="unbounded" minOccurs="0"/>
          <xsd:element name="File" type="FileOperandType" maxOccurs="unbounded" minOccurs="0"/>
          <xsd:element name="Procedure" type="ProcedureReferenceType" maxOccurs="unbounded" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute name="Type" type="UseDebugTypeEnumType"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:simpleType name="UsageEnumType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="DISPLAY"/>
      <xsd:enumeration value="BINARY"/>
      <xsd:enumeration value="COMP_1"/>
      <xsd:enumeration value="COMP_2"/>
      <xsd:enumeration value="COMP_3"/>
      <xsd:enumeration value="COMP_5"/>
      <xsd:enumeration value="COMP_6"/>
      <xsd:enumeration value="COMP_X"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="PredefinedEnumType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="BinaryChar"/>
      <xsd:enumeration value="BinaryShort"/>
      <xsd:enumeration value="BinaryLong"/>
      <xsd:enumeration value="BinaryDouble"/>
      <xsd:enumeration value="BinaryCharUns"/>
      <xsd:enumeration value="BinaryShortUns"/>
      <xsd:enumeration value="BinaryLongUns"/>
      <xsd:enumeration value="BinaryDoubleUns"/>
      <xsd:enumeration value="Character"/>
      <xsd:enumeration value="FloatShort"/>
      <xsd:enumeration value="FloatLong"/>
      <xsd:enumeration value="ConditionValue"/>
      <xsd:enumeration value="String"/>
      <xsd:enumeration value="Decimal"/>
      <xsd:enumeration value="Object"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="UseModeEnumType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Files"/>
      <xsd:enumeration value="Extend"/>
      <xsd:enumeration value="Input"/>
      <xsd:enumeration value="I-O"/>
      <xsd:enumeration value="Output"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="UseStatementType">
    <xsd:annotation>
      <xsd:documentation>Representation of a Procedure Division USE statement</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="StatementType">
        <xsd:sequence>
          <xsd:element name="File" type="FileOperandType" maxOccurs="unbounded" minOccurs="0"/>
          <xsd:element name="UseDebug" type="UseDebugType" maxOccurs="unbounded" minOccurs="0"/>
          <xsd:element name="Report" type="DataOperandType" maxOccurs="unbounded" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute name="Beginning" type="xsd:boolean" default="false"/>
        <xsd:attribute name="Ending" type="xsd:boolean" default="false"/>
        <xsd:attribute name="Global" type="xsd:boolean" default="false"/>
        <!-- checker sets OfFile="true" and OfReel="true" when neither FILE nor REEL specified (because this is default) -->
        <xsd:attribute name="OfFile" type="xsd:boolean" default="false"/>
        <xsd:attribute name="OfReel" type="xsd:boolean" default="false"/>
        <xsd:attribute name="UseMode" type="UseModeEnumType"/>
        <xsd:attribute name="UseType" type="UseTypeEnumType"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:simpleType name="UseTypeEnumType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Absent"/>
      <xsd:enumeration value="Debugging"/>
      <xsd:enumeration value="FileError"/>
      <xsd:enumeration value="Label"/>
      <xsd:enumeration value="Object"/>
      <xsd:enumeration value="ProgramStart"/>
      <xsd:enumeration value="ProgramEnd"/>
      <xsd:enumeration value="Report"/>
      <xsd:enumeration value="Transaction"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="UTF8DataItemType">
    <xsd:annotation>
      <xsd:documentation>Representation of a COBOL UTF-8 data item</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="DataItemType">
        <xsd:sequence/>
        <xsd:attribute name="ByteLength" type="xsd:boolean" default="false"/>
        <xsd:attribute name="JustifiedRight" type="xsd:boolean" default="false"/>
        <xsd:attribute name="DynamicLength" type="xsd:boolean" default="false"/>
        <xsd:attribute name="DynamicLimit" type="xsd:integer"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="VaryingType">
    <xsd:annotation>
      <xsd:documentation>Representation of a Perform Varying phrase</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="CobolObjectType">
        <xsd:sequence>
          <xsd:element name="LoopVar" type="OperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="From" type="OperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="By" type="OperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="Until" type="ConditionType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="ThroughType">
    <xsd:annotation>
      <xsd:documentation>Representation of a Perform Varying Through phrase</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="CobolObjectType">
        <xsd:sequence>
          <xsd:element name="LoopVar" type="OperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="Key" type="OperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="Value" type="OperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="Through" type="OperandType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:simpleType name="VisibilityEnumType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="PRIVATE"/>
      <xsd:enumeration value="PUBLIC"/>
      <xsd:enumeration value="PROTECTED"/>
      <xsd:enumeration value="INTERNAL"/>
      <xsd:enumeration value="PROTECTEDINTERNAL"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="WaitACUThreadStatementType">
    <xsd:annotation>
      <xsd:documentation>Representation of a Procedure Division WAIT statement (ACU)</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="StatementType">
        <xsd:sequence>
          <xsd:element name="ForThread" type="OperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="Size" type="OperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="Status" type="OperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="Timeout" type="OperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="ThreadIn" type="OperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="OnException" type="StatementBlockType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="NotOnException" type="StatementBlockType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute name="WaitType" type="ReceiveWaitTypeEnumType"/>
        <xsd:attribute name="TestOnly" type="xsd:boolean" default="false"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="WaitStatementType">
    <xsd:annotation>
      <xsd:documentation>Representation of a Procedure Division WAIT statement (MF)</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="StatementType">
        <xsd:sequence>
          <xsd:element name="Operand" type="OperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="Returning" type="OperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="Status" type="OperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="OnException" type="StatementBlockType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="NotOnException" type="StatementBlockType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="WhenCompiledOperandType">
    <xsd:annotation>
      <xsd:documentation>Representation of a WHEN-COMPILED operand</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="OperandType">
        <xsd:sequence/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="WithFillerClauseType">
    <xsd:annotation>
      <xsd:documentation>Representation of a WITH FILLER phrase within a Procedure Division statement</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ClauseType">
        <xsd:sequence/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="WorkingStorageSectionType">
    <xsd:annotation>
      <xsd:documentation>Representation of a COBOL Working-Storage Section</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ReservedSectionType">
        <xsd:sequence>
          <xsd:element name="DataItems" type="DataItemType" maxOccurs="unbounded" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="ObjectStorageSectionType">
    <xsd:annotation>
      <xsd:documentation>Representation of a COBOL Object-Storage Section</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ReservedSectionType">
        <xsd:sequence>
          <xsd:element name="DataItems" type="DataItemType" maxOccurs="unbounded" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="WriteAdvancingClauseType">
    <xsd:annotation>
      <xsd:documentation>Representation of an ADVANCING phrase within a WRITE statement</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ClauseType">
        <xsd:sequence>
          <xsd:element name="Lines" type="OperandType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute name="BeforeAfter" type="BeforeAfterEnumType"/>
        <xsd:attribute name="AdvancingType" type="WriteAdvancingTypeEnumType"/>
        <xsd:attribute name="MnemonicValue" type="xsd:integer"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:simpleType name="WriteAdvancingTypeEnumType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="LINE"/>
      <xsd:enumeration value="PAGE"/>
      <xsd:enumeration value="TAB"/>
      <xsd:enumeration value="MNEMONIC"/>
      <xsd:enumeration value="UNSET"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="WriteCollectionStatementType">
    <xsd:annotation>
      <xsd:documentation>Representation of a Procedure Division WRITE Collection statement</xsd:documentation>
