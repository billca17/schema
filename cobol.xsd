    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="StatementType">
        <xsd:sequence>
          <xsd:element name="Operands" type="OperandType" maxOccurs="unbounded" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="InspectTallyingReplacingStatementType">
    <xsd:annotation>
      <xsd:documentation>Representation of a Procedure Division INSPECT TALLYING REPLACING statement</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="InspectStatementType">
        <xsd:sequence>
          <xsd:element name="TallyingClauses" type="InspectClauseType" maxOccurs="unbounded" minOccurs="0"/>
          <xsd:element name="ReplacingClauses" type="InspectClauseType" maxOccurs="unbounded" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="InspectTallyingStatementType">
    <xsd:annotation>
      <xsd:documentation>Representation of a Procedure Division INSPECT TALLYING statement</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="InspectStatementType">
        <xsd:sequence>
          <xsd:element name="TallyingClauses" type="InspectClauseType" maxOccurs="unbounded" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="InstanceConditionType">
    <xsd:annotation>
      <xsd:documentation>Representation of an instance of condition</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ConditionType">
        <xsd:sequence>
          <xsd:element name="Subject" type="OperandType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="ContainsConditionType">
    <xsd:annotation>
      <xsd:documentation>Representation of a contains condition</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ConditionType">
        <xsd:sequence>
          <xsd:element name="Subject" type="OperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="Object" type="OperandType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:simpleType name="IntensityEnumType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="High"/>
      <xsd:enumeration value="Low"/>
      <xsd:enumeration value="Standard"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="IntensityType">
    <xsd:annotation>
      <xsd:documentation>Representation of an Accept/Display intensity</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ASTMObjectType">
        <xsd:sequence/>
        <xsd:attribute name="Type" type="IntensityEnumType"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:simpleType name="InvokeMethodTypeEnumType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="IntrinsicFunction"/>
      <xsd:enumeration value="ISOFunction"/>
      <xsd:enumeration value="InlineMethod"/>
      <xsd:enumeration value="BooleanFunction"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="InvokeStatementType">
    <xsd:annotation>
      <xsd:documentation>Representation of a Procedure Division INVOKE statement</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="StatementType">
        <xsd:sequence>
          <xsd:element name="Object" type="OperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="Template" type="OperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="Name" type="OperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="Parameters" type="FunctionParameterType" maxOccurs="unbounded" minOccurs="0"/>
          <xsd:element name="Returning" type="OperandType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute name="Await" type="xsd:boolean" default="false"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="IOControlType">
    <xsd:annotation>
      <xsd:documentation>Representation of a COBOL I-O-Control paragraph</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="CobolObjectType">
        <xsd:sequence>
          <xsd:element name="Same" type="SameRecordAreaType" maxOccurs="unbounded" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="JsonParseStatementType">
    <xsd:annotation>
      <xsd:documentation>Representation of Json Parse</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="StatementType">
        <xsd:sequence>
          <xsd:element name="Target" type="OperandType" maxOccurs="1" minOccurs="1"/>
          <xsd:element name="Source" type="OperandType" maxOccurs="1" minOccurs="1"/>
          <xsd:element name="Name"   type="JXmlNameType" maxOccurs="unbounded" minOccurs="0"/>
          <xsd:element name="Suppress" type="JXmlSuppressType" maxOccurs="unbounded" minOccurs="0"/>
          <xsd:element name="Converting"   type="JsonConvertType" maxOccurs="unbounded" minOccurs="0"/>
          <xsd:element name="OnException" type="StatementBlockType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="NotOnException" type="StatementBlockType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute name="Detail" type="xsd:boolean" default="false"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="JXmlNameType">
    <xsd:annotation>
      <xsd:documentation>Xml/Json Parse/Generate Name clause</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="OperandType">
        <xsd:sequence>
          <xsd:element name="Name" type="OperandType" maxOccurs="1" minOccurs="1"/>
          <xsd:element name="Is" type="OperandType" maxOccurs="1" minOccurs="1"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="JXmlSuppressType">
    <xsd:annotation>
      <xsd:documentation>Xml/Json Generate Suppress clause</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="OperandType">
        <xsd:sequence>
          <xsd:element name="SuppressName" type="OperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="Category" type="SuppressCategoryOperandType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute name="WhenZero" type="xsd:boolean" default="false"/>
        <xsd:attribute name="WhenSpace" type="xsd:boolean" default="false"/>
        <xsd:attribute name="WhenLowValue" type="xsd:boolean" default="false"/>
        <xsd:attribute name="WhenHighValue" type="xsd:boolean" default="false"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="JsonConvertType">
    <xsd:annotation>
      <xsd:documentation>Json Parse/Generate Converting clause</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="OperandType">
        <xsd:sequence>
          <xsd:element name="Name"  type="OperandType" maxOccurs="1" minOccurs="1"/>
          <xsd:element name="Using" type="OperandType" maxOccurs="2" minOccurs="1"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="JsonGenerateStatementType">
    <xsd:annotation>
      <xsd:documentation>Representation of Json Generate</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="StatementType">
        <xsd:sequence>
          <xsd:element name="Target" type="OperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="Source" type="OperandType" maxOccurs="1" minOccurs="1"/>
          <xsd:element name="Count"  type="OperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="Name"   type="JXmlNameType" maxOccurs="unbounded" minOccurs="0"/>
          <xsd:element name="Suppress" type="JXmlSuppressType" maxOccurs="unbounded" minOccurs="0"/>
          <xsd:element name="Converting"   type="JsonConvertType" maxOccurs="unbounded" minOccurs="0"/>
          <xsd:element name="OnException" type="StatementBlockType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="NotOnException" type="StatementBlockType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="LengthAnOperandType">
    <xsd:annotation>
      <xsd:documentation>Representation of a LENGTH-AN intrinsic function operand</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="OperandType">
        <xsd:sequence>
          <xsd:element name="Operand" type="OperandType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="LengthOperandType">
    <xsd:annotation>
      <xsd:documentation>Representation of a LENGTH OF operand</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="OperandType">
        <xsd:sequence>
          <xsd:element name="Operand" type="OperandType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="Level78DataItemType">
    <xsd:annotation>
      <xsd:documentation>Representation of a COBOL Level 78 item</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="DataItemType">
        <xsd:sequence/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="LinageClauseType">
    <xsd:annotation>
      <xsd:documentation>Representation of a COBOL Linage clause</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="CobolObjectType">
        <xsd:sequence>
          <xsd:element name="Lines" type="OperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="Footing" type="OperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="LinesTop" type="OperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="LinesBottom" type="OperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="DataItems" type="DataItemType" maxOccurs="unbounded" minOccurs="0"/>  <!-- linage-counter -->
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="LineSequentialFileType">
    <xsd:annotation>
      <xsd:documentation>Representation of a COBOL Line Sequential File</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="CobolFileType">
        <xsd:sequence/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="LinkageSectionType">
    <xsd:annotation>
      <xsd:documentation>Representation of a COBOL Linkage Section</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ReservedSectionType">
        <xsd:sequence>
          <xsd:element name="DataItems" type="DataItemType" maxOccurs="unbounded" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="LiteralOperandType">
    <xsd:annotation>
      <xsd:documentation>Representation of a literal operand</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="OperandType">
        <xsd:sequence/>
        <xsd:attribute name="Value" type="xsd:string"/>
        <xsd:attribute name="Type" type="LiteralTypeEnumType"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:simpleType name="LiteralTypeEnumType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="ALPHA"/>
      <xsd:enumeration value="FIG"/>
      <xsd:enumeration value="ALL"/>
      <xsd:enumeration value="INTEGER"/>
      <xsd:enumeration value="NUMERIC"/>
      <xsd:enumeration value="FLOATING"/>
      <xsd:enumeration value="BOOLEAN"/>
      <xsd:enumeration value="ALLBOOLEAN"/>
      <xsd:enumeration value="UTF8"/>
      <xsd:enumeration value="ALLUTF8"/>
      <xsd:enumeration value="NATIONAL"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="LManagerNameEnumType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="lm-resize"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="LocalStorageSectionType">
    <xsd:annotation>
      <xsd:documentation>Representation of a COBOL Local-Storage Section</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ReservedSectionType">
        <xsd:sequence>
          <xsd:element name="DataItems" type="DataItemType" maxOccurs="unbounded" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="LockThreadStatementType">
    <xsd:annotation>
      <xsd:documentation>Representation of a Procedure Division LOCK THREAD statement (ACU)</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="StatementType">
        <xsd:sequence/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:simpleType name="LockModeEnumType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="NOLOCK"/>
      <xsd:enumeration value="LOCK"/>
      <xsd:enumeration value="KEPTLOCK"/>
      <xsd:enumeration value="WAIT"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="MemorySizeType">
    <xsd:annotation>
      <xsd:documentation>Representation of an Object-Computer Memory Size clause</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="CobolObjectType">
        <xsd:sequence/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="MergeStatementType">
    <xsd:annotation>
      <xsd:documentation>Representation of a Procedure Division MERGE statement</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="StatementType">
        <xsd:sequence>
          <xsd:element name="File" type="FileOperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="Keys" type="SortKeyType" maxOccurs="unbounded" minOccurs="0"/>
          <xsd:element name="Using" type="FileOperandType" maxOccurs="unbounded" minOccurs="0"/>
          <xsd:element name="OutputStart" type="ProcedureReferenceType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="OutputEnd" type="ProcedureReferenceType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="Giving" type="FileOperandType" maxOccurs="unbounded" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute name="KeyArea" type="xsd:boolean" default="false"/>
        <xsd:attribute name="CollatingSequence" type="xsd:string"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="MethodType">
    <xsd:annotation>
      <xsd:documentation>Representation of a COBOL Method</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="CobolProcedureType">
        <xsd:sequence>
          <xsd:element name="GenericParameter" type="GenericParameterType" maxOccurs="unbounded" minOccurs="0"/>
          <xsd:element name="CustomAttributes" type="CustomAttributeType" maxOccurs="unbounded" minOccurs="0"/>
          <xsd:element name="ConstantDivision" type="ConstantDivisionType" maxOccurs="1" minOccurs="0"/> 
          <xsd:element name="IdDivision" type="IdentificationDivisionType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="EnvironmentDivision" type="EnvironmentDivisionType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="DataDivision" type="DataDivisionType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="ProcedureDivision" type="ProcedureDivisionType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="EndMarker" type="EndMethodType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute name="Visibility" type="VisibilityEnumType"/>
        <xsd:attribute name="Property" type="PropertyEnumType"/>
        <xsd:attribute name="Static" type="xsd:boolean" default="false"/>
        <xsd:attribute name="Final" type="xsd:boolean" default="false"/>
        <xsd:attribute name="Override" type="xsd:boolean" default="false"/>
        <xsd:attribute name="Abstract" type="xsd:boolean" default="false"/>
        <xsd:attribute name="Redefine" type="xsd:boolean" default="false"/>
        <xsd:attribute name="Sync" type="xsd:boolean" default="false"/>
        <xsd:attribute name="Async" type="xsd:boolean" default="false"/>
        <xsd:attribute name="Extension" type="xsd:boolean" default="false"/>
        <xsd:attribute name="ForInterface" type="xsd:string"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="IteratorType">
    <xsd:annotation>
      <xsd:documentation>Representation of a COBOL Iterator</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="CobolProcedureType">
        <xsd:sequence>
          <xsd:element name="GenericParameter" type="GenericParameterType" maxOccurs="unbounded" minOccurs="0"/>
          <xsd:element name="CustomAttributes" type="CustomAttributeType" maxOccurs="unbounded" minOccurs="0"/>
          <xsd:element name="IdDivision" type="IdentificationDivisionType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="EnvironmentDivision" type="EnvironmentDivisionType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="DataDivision" type="DataDivisionType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="ProcedureDivision" type="ProcedureDivisionType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="EndMarker" type="EndIteratorType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute name="Visibility" type="VisibilityEnumType"/>
        <xsd:attribute name="Property" type="PropertyEnumType"/>
        <xsd:attribute name="Static" type="xsd:boolean" default="false"/>
        <xsd:attribute name="Final" type="xsd:boolean" default="false"/>
        <xsd:attribute name="Override" type="xsd:boolean" default="false"/>
        <xsd:attribute name="Abstract" type="xsd:boolean" default="false"/>
        <xsd:attribute name="Redefine" type="xsd:boolean" default="false"/>
        <xsd:attribute name="Sync" type="xsd:boolean" default="false"/>
        <xsd:attribute name="Extension" type="xsd:boolean" default="false"/>
        <xsd:attribute name="ForInterface" type="xsd:string"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="OperatorOverloadType">
    <xsd:annotation>
      <xsd:documentation>Representation of a COBOL Operator Overload</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="CobolProcedureType">
        <xsd:sequence>
          <xsd:element name="GenericParameter" type="GenericParameterType" maxOccurs="unbounded" minOccurs="0"/>
          <xsd:element name="CustomAttributes" type="CustomAttributeType" maxOccurs="unbounded" minOccurs="0"/>
          <xsd:element name="IdDivision" type="IdentificationDivisionType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="EnvironmentDivision" type="EnvironmentDivisionType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="DataDivision" type="DataDivisionType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="ProcedureDivision" type="ProcedureDivisionType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="EndMarker" type="EndOperatorType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute name="Visibility" type="VisibilityEnumType"/>
        <xsd:attribute name="Property" type="PropertyEnumType"/>
        <xsd:attribute name="Static" type="xsd:boolean" default="false"/>
        <xsd:attribute name="Final" type="xsd:boolean" default="false"/>
        <xsd:attribute name="Override" type="xsd:boolean" default="false"/>
        <xsd:attribute name="Abstract" type="xsd:boolean" default="false"/>
        <xsd:attribute name="Redefine" type="xsd:boolean" default="false"/>
        <xsd:attribute name="Sync" type="xsd:boolean" default="false"/>
        <xsd:attribute name="Extension" type="xsd:boolean" default="false"/>
        <xsd:attribute name="ForInterface" type="xsd:string"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="ParmSectOperandType">
    <xsd:annotation>
      <xsd:documentation>Representation of an parameterized section</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="OperandType">
        <xsd:sequence>
          <xsd:element name="Name" type="OperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="ParmSect" type="ProcedureReferenceType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="Parameters" type="FunctionParameterType" maxOccurs="unbounded" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="MethodOperandType">
    <xsd:annotation>
      <xsd:documentation>Representation of an inline method</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="OperandType">
        <xsd:sequence>
          <xsd:element name="Object" type="OperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="Name" type="OperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="Parameters" type="FunctionParameterType" maxOccurs="unbounded" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute name="Await" type="xsd:boolean" default="false"/>
        <xsd:attribute name="InvokeMethodType" type="InvokeMethodTypeEnumType"/>
        <xsd:attribute name="Constructor" type="xsd:boolean" default="false"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="MethodGroupOperandType">
    <xsd:annotation>
      <xsd:documentation>Representation of a method group (used with delegates)</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="OperandType">
        <xsd:sequence>
          <xsd:element name="Object" type="OperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="Name" type="OperandType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="ModifyInquireIndexType">
    <xsd:complexContent>
      <xsd:extension base="CobolObjectType">
        <xsd:sequence>
          <xsd:element name="ModifyInquireIndex" type="OperandType" maxOccurs="unbounded" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="ModifyInquireStatementType" abstract="true">
    <xsd:annotation>
      <xsd:documentation>Representation of a Procedure Division MODIFY or INQUIRE statement (ACU)</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="StatementType">
        <xsd:sequence>
          <xsd:element name="Operand" type="OperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="Indexes" type="ModifyInquireIndexType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="EventStart" type="ProcedureReferenceType" maxOccurs="1" minOccurs="0"/> <!-- only modify -->
          <xsd:element name="EventEnd" type="ProcedureReferenceType" maxOccurs="1" minOccurs="0"/> <!-- only modify -->
          <xsd:element name="With" type="AcceptDisplayWithClauseType" maxOccurs="unbounded" minOccurs="0"/>
          <xsd:element name="ControlProperties" type="ControlPropertiesType" minOccurs="0" maxOccurs="1"/>
          <xsd:element name="Style" type="ControlStylesType" minOccurs="0" maxOccurs="1"/>
        </xsd:sequence>
        <xsd:attribute name="Control" type="xsd:boolean" default="false"/>
        <xsd:attribute name="ControlHandle" type="xsd:boolean" default="false"/>
        <xsd:attribute name="ControlItem" type="xsd:boolean" default="false"/>
        <xsd:attribute name="Window" type="xsd:boolean" default="false"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="ModifyStatementType">
    <xsd:annotation>
      <xsd:documentation>Representation of a Procedure Division MODIFY statement (ACU)</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ModifyInquireStatementType">
        <xsd:sequence>
          <xsd:element name="NotStyle" type="ControlStylesType" minOccurs="0" maxOccurs="1"/>
          <xsd:element name="OnException" type="StatementBlockType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="NotOnException" type="StatementBlockType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="MonitorPointerDataItemType">
    <xsd:annotation>
      <xsd:documentation>Representation of a COBOL Monitor-Pointer data item</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
