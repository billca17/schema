  <xsd:complexType name="CDType">
    <xsd:annotation>
      <xsd:documentation>Representation of a COBOL CD</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="CobolItemType">
        <xsd:sequence>
          <xsd:element name="CD" type="CDType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="DataItems" type="DataItemType" maxOccurs="unbounded" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute name="For" type="CDForEnumType"/>
        <xsd:attribute name="Initial" type="xsd:boolean" default="false"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="ChainStatementType">
    <xsd:annotation>
      <xsd:documentation>Representation of a Procedure Division CHAIN statement</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="StatementType">
        <xsd:sequence>
          <xsd:element name="Name" type="OperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="Parameters" type="FunctionParameterType" maxOccurs="unbounded" minOccurs="0"/>
          <xsd:element name="Returning" type="OperandType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute name="CallConvention" type="xsd:integer"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="ClassConditionType">
    <xsd:annotation>
      <xsd:documentation>Representation of a class condition</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ConditionType">
        <xsd:sequence>
          <xsd:element name="Subject" type="OperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="UserClass" type="DataOperandType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute name="AllowedCharacters" type="ClassConditionEnumType" default="UserDefined"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:simpleType name="ClassConditionEnumType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="ALPHABETIC"/>
      <xsd:enumeration value="ALPHABETICLOWER"/>
      <xsd:enumeration value="ALPHABETICUPPER"/>
      <xsd:enumeration value="DBCS"/>
      <xsd:enumeration value="KANJI"/>
      <xsd:enumeration value="JAPANESE"/>
      <xsd:enumeration value="NUMERIC"/>
      <xsd:enumeration value="OMITTED"/>
      <xsd:enumeration value="UserDefined"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="ClassNameType">
    <xsd:annotation>
      <xsd:documentation>Representation of a COBOL SPECIAL-NAMES class-name</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="CobolItemType">
        <xsd:sequence/>
        <xsd:attribute name="AllowedCharacters" type="xsd:string" default=""/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="ClassNameOperandType">
    <xsd:annotation>
      <xsd:documentation>Representation of an explicit class name operand</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="OperandType">
        <xsd:sequence/>
        <xsd:attribute name="TypeName" type="xsd:string"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="ClauseType" abstract="true">
    <xsd:annotation>
      <xsd:documentation>Representation of a phrase within a Procedure Division statement</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="CobolObjectType">
        <xsd:sequence/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="CloseOperandType">
    <xsd:annotation>
      <xsd:documentation>Representation of a Procedure Division CLOSE statement operand</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="CobolObjectType">
        <xsd:sequence>
          <xsd:element name="File" type="FileOperandType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute name="Lock" type="xsd:boolean" default="false"/>
        <xsd:attribute name="NoRewind" type="xsd:boolean" default="false"/>
        <xsd:attribute name="ReelNoRewind" type="xsd:boolean" default="false"/>
        <xsd:attribute name="ReelForRemoval" type="xsd:boolean" default="false"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="CloseStatementType">
    <xsd:annotation>
      <xsd:documentation>Representation of a Procedure Division CLOSE statement</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="StatementType">
        <xsd:choice>
          <xsd:element name="Files" type="CloseOperandType" maxOccurs="unbounded" minOccurs="0"/>
          <xsd:element name="Operand" type="OperandType" maxOccurs="1" minOccurs="0"/>
        </xsd:choice>
        <xsd:attribute name="CloseType" type="CloseTypeEnum" default="Files"/>
        <xsd:attribute name="NoDisplay" type="xsd:boolean" default="false"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:simpleType name="CloseTypeEnum">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Files"/>
      <xsd:enumeration value="Window"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="CobolClassType">
    <xsd:annotation>
      <xsd:documentation>Representation of a COBOL Class Program</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="CobolTypeType">
        <xsd:sequence>
          <xsd:element name="ConstantDivision" type="ConstantDivisionType" maxOccurs="1" minOccurs="0"/> 
          <xsd:element name="Implements" type="TypeNameType" maxOccurs="unbounded" minOccurs="0"/>
          <xsd:element name="IdDivision" type="IdentificationDivisionType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="EnvironmentDivision" type="EnvironmentDivisionType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="DataDivision" type="DataDivisionType" maxOccurs="1" minOccurs="0"/>
          <xsd:choice maxOccurs="unbounded" minOccurs="0">
            <xsd:element name="Methods" type="MethodType"/>
            <xsd:element name="OperatorOverload" type="OperatorOverloadType"/>
            <xsd:element name="Iterator" type="IteratorType"/>
            <xsd:element name="Indexers" type="CobolIndexerType"/>
            <xsd:element name="Properties" type="CobolPropertyType"/>
          </xsd:choice>
          <xsd:element name="NestedTypes" type="CobolTypeType" maxOccurs="unbounded" minOccurs="0"/>
          <xsd:element name="EndMarker" type="EndClassType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute name="ValueType" type="xsd:boolean" default="false"/>
        <xsd:attribute name="Partial" type="xsd:boolean" default="false"/>
        <xsd:attribute name="Inherits" type="xsd:string"/>
        <xsd:attribute name="Visibility" type="VisibilityEnumType"/>
        <xsd:attribute name="Static" type="xsd:boolean" default="false"/>
        <xsd:attribute name="Abstract" type="xsd:boolean" default="false"/>
        <xsd:attribute name="Final" type="xsd:boolean" default="false"/>
        <xsd:attribute name="Serializable" type="xsd:boolean" default="false"/>
        <xsd:attribute name="SharingParent" type="xsd:boolean" default="false"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="CobolValueTypeType">
    <xsd:annotation>
      <xsd:documentation>Representation of a COBOL ValueType Program</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="CobolTypeType">
        <xsd:sequence>
          <xsd:element name="Implements" type="TypeNameType" maxOccurs="unbounded" minOccurs="0"/>
          <xsd:element name="IdDivision" type="IdentificationDivisionType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="EnvironmentDivision" type="EnvironmentDivisionType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="DataDivision" type="DataDivisionType" maxOccurs="1" minOccurs="0"/>
          <xsd:choice maxOccurs="unbounded" minOccurs="0">
            <xsd:element name="Methods" type="MethodType"/>
            <xsd:element name="OperatorOverload" type="OperatorOverloadType"/>
            <xsd:element name="Iterator" type="IteratorType"/>
            <xsd:element name="Indexers" type="CobolIndexerType"/>
            <xsd:element name="Properties" type="CobolPropertyType" maxOccurs="unbounded" minOccurs="0"/>
          </xsd:choice>
          <xsd:element name="NestedTypes" type="CobolTypeType" maxOccurs="unbounded" minOccurs="0"/>
          <xsd:element name="EndMarker" type="EndValueTypeType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute name="ValueType" type="xsd:boolean" default="false"/>
        <xsd:attribute name="Partial" type="xsd:boolean" default="false"/>
        <xsd:attribute name="Inherits" type="xsd:string"/>
        <xsd:attribute name="Visibility" type="VisibilityEnumType"/>
        <xsd:attribute name="Static" type="xsd:boolean" default="false"/>
        <xsd:attribute name="Abstract" type="xsd:boolean" default="false"/>
        <xsd:attribute name="Final" type="xsd:boolean" default="false"/>
        <xsd:attribute name="Serializable" type="xsd:boolean" default="false"/>
        <xsd:attribute name="SharingParent" type="xsd:boolean" default="false"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="CobolComponentType">
    <xsd:annotation>
      <xsd:documentation>Representation of a COBOL Component</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="CobolObjectType">
        <xsd:sequence>
          <xsd:element name="Options" type="OptionsType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="Messages" type="CompilerMessageType" maxOccurs="unbounded" minOccurs="0"/>
          <xsd:element name="CommentStats" type="CommentStatsType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="Comments" type="CommentsType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="SourceStructure" type="SourceFileType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="Programs" type="CobolStructType" maxOccurs="unbounded" minOccurs="1"/>
        </xsd:sequence>
        <xsd:attribute name="CompileTimeStamp" type="xsd:dateTime"/>
        <xsd:attribute name="version" type="xsd:string" default="0.0"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="CobolDelegateType">
    <xsd:annotation>
      <xsd:documentation>Representation of a COBOL Delegate</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="CobolTypeType">
        <xsd:sequence>
          <xsd:element name="IdDivision" type="IdentificationDivisionType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="EnvironmentDivision" type="EnvironmentDivisionType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="DataDivision" type="DataDivisionType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="ProcedureDivision" type="ProcedureDivisionType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="EndMarker" type="EndDelegateType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute name="Visibility" type="VisibilityEnumType"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="CobolEnumType">
    <xsd:annotation>
      <xsd:documentation>Representation of a COBOL Enum</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="CobolTypeType">
        <xsd:sequence>
          <xsd:element name="ConstantDivision" type="ConstantDivisionType" maxOccurs="1" minOccurs="0"/> 
          <xsd:element name="IdDivision" type="IdentificationDivisionType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="DataDivision" type="DataDivisionType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="EndMarker" type="EndEnumType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute name="Visibility" type="VisibilityEnumType"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="CobolFDBlockPhraseType">
    <xsd:annotation>
      <xsd:documentation>Representation of the BLOCK CONTAINS phrase in a Cobol FD</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ClauseType">
        <xsd:attribute name="ContainsType" type="BlockContainsEnumType"/>
        <xsd:attribute name="Min" type="xsd:integer"/>
        <xsd:attribute name="Max" type="xsd:integer"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="CobolFDRecordPhraseType">
    <xsd:annotation>
      <xsd:documentation>Representation of the record phrase in a Cobol FD/SD</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ClauseType">
        <xsd:sequence>
          <xsd:element name="RecordDepending" type="DataOperandType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute name="PhraseType" type="RecordPhraseEnumType"/>
        <xsd:attribute name="Min" type="xsd:integer"/>
        <xsd:attribute name="Max" type="xsd:integer"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="CobolFileType" abstract="true">
    <xsd:annotation>
      <xsd:documentation>Representation of a COBOL File</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="CobolItemType">
        <xsd:sequence>
          <xsd:element name="Assign" type="CobolFileAssignType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="Status" type="DataOperandType" maxOccurs="unbounded" minOccurs="0"/>
          <xsd:element name="Locking" type="CobolFileLockingType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="Password" type="OperandType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute name="Access" type="FileAccessEnumType"/>
        <xsd:attribute name="FileNameColumn" type="xsd:int" default="0"/>
        <xsd:attribute name="FileNameLine" type="xsd:int" default="0"/>
        <xsd:attribute name="Optional" type="xsd:boolean" default="false"/>
        <xsd:attribute name="Organization" type="FileOrganizationEnumType"/>
        <xsd:attribute name="Sharing" type="CobolFileSharingEnumType"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:simpleType name="CobolFileAllowingEnumType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="All"/>
      <xsd:enumeration value="NoOthers"/>
      <xsd:enumeration value="Readers"/>
      <xsd:enumeration value="Updaters"/>
      <xsd:enumeration value="Writers"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="CobolFileAssignType">
    <xsd:annotation>
      <xsd:documentation>Representation of the ASSIGN clause in a COBOL file-control-entry</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ClauseType">
        <xsd:sequence>
          <xsd:element name="Operand" type="OperandType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute name="AssignMode" type="CobolSelectModeEnumType"/>
        <xsd:attribute name="AssignType" type="CobolSelectTypeEnumType"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="CobolFileCollatingType">
    <xsd:annotation>
      <xsd:documentation>Representation of the COLLATING clause in a COBOL indexed file-control-entry</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ClauseType">
        <xsd:sequence/>
        <xsd:attribute name="CIDRef" type="xsd:IDREF"/>  <!-- Reference to alphabet that defines collating sequence -->
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="CobolFileDescriptorType" abstract="true">
    <xsd:annotation>
      <xsd:documentation>Representation of a COBOL FD or SD</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="CobolObjectType">
        <xsd:sequence>
          <xsd:element name="File" type="FileOperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="BlockPhrase" type="CobolFDBlockPhraseType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="RecordPhrase" type="CobolFDRecordPhraseType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute name="External" type="xsd:boolean" default="false"/>
        <xsd:attribute name="Global" type="xsd:boolean" default="false"/>
        <xsd:attribute name="Max" type="xsd:integer"/>
        <xsd:attribute name="Min" type="xsd:integer"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="CobolFileLockingType">
    <xsd:annotation>
      <xsd:documentation>Representation of the LOCK clause in a COBOL file-control-entry</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ClauseType">
        <xsd:sequence/>
        <xsd:attribute name="LockMode" type="CobolSelectLockModeEnumType"/>
        <xsd:attribute name="Rollback" type="xsd:boolean" default="false"/>
        <xsd:attribute name="MultipleRecords" type="xsd:boolean" default="false"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="CobolFilePaddingType">
    <xsd:annotation>
      <xsd:documentation>Representation of the PADDING clause in a COBOL file-control-entry</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ClauseType">
        <xsd:sequence>
          <xsd:element name="PaddingOperand" type="OperandType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute name="PaddingChar" type="xsd:string"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="CobolFileReserveType">
    <xsd:annotation>
      <xsd:documentation>Representation of the RESERVE clause in a COBOL file-control-entry</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ClauseType">
        <xsd:sequence/>
        <xsd:attribute name="ReserveArea" type="xsd:boolean" default="false"/>
        <xsd:attribute name="AreaCount" type="xsd:integer"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:simpleType name="CobolFileSharingEnumType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="ALLOTHER"/>
      <xsd:enumeration value="NOOTHER"/>
      <xsd:enumeration value="READONLY"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="CobolFunctionType">
    <xsd:annotation>
      <xsd:documentation>Representation of a COBOL Function</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="CobolProcedureType">
        <xsd:sequence>
          <xsd:element name="IdDivision" type="IdentificationDivisionType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="EnvironmentDivision" type="EnvironmentDivisionType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="DataDivision" type="DataDivisionType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="ProcedureDivision" type="ProcedureDivisionType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="EndMarker" type="EndFunctionType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="CobolFunctionPrototypeType">
    <xsd:annotation>
      <xsd:documentation>Representation of a COBOL Function Prototype</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="CobolProcedureType">
        <xsd:sequence>
          <xsd:element name="IdDivision" type="IdentificationDivisionType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="EnvironmentDivision" type="EnvironmentDivisionType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="DataDivision" type="DataDivisionType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="ProcedureDivision" type="ProcedureDivisionType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="EndMarker" type="EndFunctionType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="CobolIndexerType">
    <xsd:annotation>
      <xsd:documentation>Representation of a COBOL Indexer Program</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="CobolTypeType">
        <xsd:sequence>
          <xsd:element name="Indexers" type="TypeNameType" maxOccurs="unbounded" minOccurs="0"/>
          <xsd:element name="IdDivision" type="IdentificationDivisionType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="EnvironmentDivision" type="EnvironmentDivisionType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="DataDivision" type="DataDivisionType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="ProcedureDivision" type="PropertyProcedureType" maxOccurs="unbounded" minOccurs="0"/>
          <xsd:element name="EndMarker" type="EndIndexerType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute name="Visibility" type="VisibilityEnumType"/>
        <xsd:attribute name="Abstract" type="xsd:boolean" default="false"/> 
        <xsd:attribute name="Override" type="xsd:boolean" default="false"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="CobolPropertyType">
    <xsd:annotation>
      <xsd:documentation>Representation of a COBOL Property Program</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="CobolTypeType">
        <xsd:sequence>
          <xsd:element name="Properties" type="TypeNameType" maxOccurs="unbounded" minOccurs="0"/>
          <xsd:element name="CustomAttributes" type="CustomAttributeType" maxOccurs="unbounded" minOccurs="0"/>
          <xsd:element name="IdDivision" type="IdentificationDivisionType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="EnvironmentDivision" type="EnvironmentDivisionType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="DataDivision" type="DataDivisionType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="ProcedureDivision" type="PropertyProcedureType" maxOccurs="unbounded" minOccurs="0"/>
          <xsd:element name="EndMarker" type="EndPropertyType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute name="Visibility" type="VisibilityEnumType"/>
        <xsd:attribute name="Override" type="xsd:boolean" default="false"/>
        <xsd:attribute name="ForInterface" type="xsd:string"/>
        <xsd:attribute name="Static" type="xsd:boolean" default="false"/>        
        <xsd:attribute name="Abstract" type="xsd:boolean" default="false"/>
        <xsd:attribute name="Async" type="xsd:boolean" default="false"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="CobolAttributeType">
    <xsd:annotation>
      <xsd:documentation>Representation of a COBOL Attribute-id Program</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="CobolTypeType">
        <xsd:sequence>
          <xsd:element name="Implements" type="TypeNameType" maxOccurs="unbounded" minOccurs="0"/>
          <xsd:element name="IdDivision" type="IdentificationDivisionType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="EnvironmentDivision" type="EnvironmentDivisionType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="DataDivision" type="DataDivisionType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="Methods" type="MethodType" maxOccurs="unbounded" minOccurs="0"/>
          <xsd:element name="EndMarker" type="EndAttributeType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute name="Visibility" type="VisibilityEnumType"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="CobolInterfaceType">
    <xsd:annotation>
      <xsd:documentation>Representation of a COBOL Interface Program</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="CobolTypeType">
        <xsd:sequence>
          <xsd:element name="ConstantDivision" type="ConstantDivisionType" maxOccurs="1" minOccurs="0"/> 
          <xsd:element name="Implements" type="TypeNameType" maxOccurs="unbounded" minOccurs="0"/>
          <xsd:element name="IdDivision" type="IdentificationDivisionType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="EnvironmentDivision" type="EnvironmentDivisionType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="DataDivision" type="DataDivisionType" maxOccurs="1" minOccurs="0"/>
          <xsd:choice maxOccurs="unbounded" minOccurs="0">
            <xsd:element name="Methods" type="MethodType"/>
            <xsd:element name="Indexers" type="CobolIndexerType"/>
            <xsd:element name="Iterator" type="IteratorType"/>
            <xsd:element name="Properties" type="CobolPropertyType"/>
          </xsd:choice>
          <xsd:element name="EndMarker" type="EndInterfaceType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
