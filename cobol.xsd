  <xsd:complexType name="AcceptStatementType">
    <xsd:annotation>
      <xsd:documentation>Representation of a Procedure Division ACCEPT statement</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="StatementType">
        <xsd:sequence>
          <xsd:element name="Operands" type="AcceptDisplayOperandsType" maxOccurs="unbounded" minOccurs="0"/>
          <xsd:element name="OnException" type="StatementBlockType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="NotOnException" type="StatementBlockType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute name="From" type="AcceptFromEnumType"/>
        <xsd:attribute name="Until" type="xsd:boolean" default="false"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:simpleType name="AccessTypeEnumType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="SOURCE"/>
      <xsd:enumeration value="SOURCEINTO"/>
      <xsd:enumeration value="TARGET"/>
      <xsd:enumeration value="MODIFY"/>
      <xsd:enumeration value="TARGETROUNDED"/>
      <xsd:enumeration value="MODIFYROUNDED"/>
      <xsd:enumeration value="REMAINDER"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="AddressOperandType">
    <xsd:annotation>
      <xsd:documentation>Representation of a ADDRESS OF operand</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="OperandType">
        <xsd:sequence>
