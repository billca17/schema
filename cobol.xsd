  <xsd:complexType name="AddStatementType">
    <xsd:annotation>
      <xsd:documentation>Representation of a Procedure Division ADD statement</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="StatementType">
        <xsd:sequence>
          <xsd:element name="Sources" type="OperandType" maxOccurs="unbounded" minOccurs="0"/>
          <xsd:element name="Targets" type="OperandType" maxOccurs="unbounded" minOccurs="0"/>
          <xsd:element name="OnSizeError" type="StatementBlockType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="NotOnSizeError" type="StatementBlockType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute name="Corresponding" type="xsd:boolean" default="false"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:simpleType name="AlphabetCollatingSequenceType">
    <xsd:annotation>
      <xsd:documentation>
        Representation of a COBOL Alphabet collating sequence defined by a literal.
        The collating sequence position (0-based) is represented as a pair of
        hexadecimal digits.  There are 256 pairs resulting in a string of 512
        characters.  The native character is mapped through this table to
        its collating sequence.  Note that multiple native characters can map
        to the same collating position because of the ALSO phrase in alphabet
        definition.  If the ALSO phrase is used, the alphabet can only be used
        as an output code-set, although at this time (2014/09/17) Visual COBOL
        does not allow a code-set defined by a literal alphabet for input or output.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:length value="512"/>
      <xsd:pattern value="[0123456789abcdef]*"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="AlphabeticDataItemType">
    <xsd:annotation>
      <xsd:documentation>Representation of a COBOL Alphabetic data item</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="DataItemType">
        <xsd:sequence/>
        <xsd:attribute name="JustifiedRight" type="xsd:boolean" default="false"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="AlphabetType">
    <xsd:annotation>
      <xsd:documentation>Representation of an ALPHABET clause in the special names paragraph</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="CobolItemType">
        <xsd:sequence/>
        <xsd:attribute name="Value" type="AlphabetValueEnumType"/>
        <xsd:attribute name="CollatingSequence" type="AlphabetCollatingSequenceType"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:simpleType name="AlphabetValueEnumType">
    <xsd:restriction base="xsd:string">
