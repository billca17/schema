  <xsd:complexType name="AcceptDisplayOperandsType">
    <xsd:annotation>
      <xsd:documentation>Representation of an operand list for a Procedure Division ACCEPT or DISPLAY statement</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="CobolObjectType">
        <xsd:sequence>
          <xsd:element name="Operand" type="OperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="With" type="AcceptDisplayWithClauseType" maxOccurs="unbounded" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:simpleType name="AcceptDisplayUnitsEnumType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Cells"/>
      <xsd:enumeration value="Pixels"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="AcceptDisplayWithClauseType">
    <xsd:annotation>
      <xsd:documentation>Representation of a WITH phrase within a Procedure Division ACCEPT, DISPLAY, INQUIRE or MODIFY statement</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="ClauseType">
        <xsd:sequence>
          <xsd:element name="WithOperand" type="OperandType" maxOccurs="1" minOccurs="0"/>
          <xsd:element name="Until" type="ConditionType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
        <xsd:attribute name="WithType" type="AcceptDisplayWithEnumType"/>
        <xsd:attribute name="Units" type="AcceptDisplayUnitsEnumType"/>
        <xsd:attribute name="ControlType" type="ControlTypeEnumType"/>
        <xsd:attribute name="Overlapped" type="xsd:boolean" default="false"/>
        <xsd:attribute name="Separate" type="xsd:boolean" default="false"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
