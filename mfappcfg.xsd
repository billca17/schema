<?xml version="1.0" encoding="utf-8" ?>
<xsd:schema elementFormDefault="qualified" 
    xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
    targetNamespace="http://xml.microfocus.com/schema/appconfig/v1.0/mfappcfg.xsd"
    xmlns="http://xml.microfocus.com/schema/appconfig/v1.0/mfappcfg.xsd">
    <!--
        ********************************************************************************
        *   Schema for COBOL runtime configuration details.                            *
        *                                                                              *
        *   Used for generation and disassembly of runtime configuration blocks built  *
        *   into IDT files, or GCF files.                                              *
        ********************************************************************************
    -->

    <!--
        *==============================================================================*
        *   The (one and only) root element. Each of the child elements are optional   *
        *   and may occur in any order. They are used to group the different types of  *
        *   configuration details (e.g. tunables, environment variables, and switches).*
        *==============================================================================*
    -->

    <xsd:element name="application_configuration">
        <xsd:complexType>
            <xsd:all>
                <xsd:element name="envvars"    type="envvarsType"    minOccurs="0" maxOccurs="1"/>
                <xsd:element name="switches"   type="switchesType"   minOccurs="0" maxOccurs="1"/>
                <xsd:element name="tunables"   type="tunablesType"   minOccurs="0" maxOccurs="1"/>
                <xsd:element name="deployment" type="deploymentType" minOccurs="0" maxOccurs="1"/>
            </xsd:all>
        </xsd:complexType>
    </xsd:element>

    <!--
        ********************************************************************************
        *   Environment variables                                                      *
        ********************************************************************************
    -->

    <!--
        *==============================================================================*
        *   Zero or many environment variables may be defined and in any order         *
        *==============================================================================*
    -->

    <xsd:complexType name="envvarsType">
        <xsd:sequence>
            <xsd:element name="envvar" type="envvarType" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
    </xsd:complexType>

    <!--
        *==============================================================================*
        *   Environment variable attributes                                            *
        *==============================================================================*
    -->

    <xsd:complexType name="envvarType">
        <xsd:attribute name="name"  type="xsd:string" use="required"/>
        <xsd:attribute name="value" type="xsd:string" use="required"/>
    </xsd:complexType>

    <!--
        ********************************************************************************
        *   Switches								        *
        ********************************************************************************
    -->

    <!--
        *==============================================================================*
        *   Zero or many switches may be defined and in any order                      *
        *==============================================================================*
    -->

    <xsd:complexType name="switchesType">
      <xsd:sequence minOccurs="0" maxOccurs="unbounded">
        <xsd:choice>
            <xsd:element name="cobol_switch" type="cobol_switch_Type" minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element name="rts_switch"   type="rts_switch_Type"   minOccurs="0" maxOccurs="unbounded"/>
        </xsd:choice>
      </xsd:sequence>
    </xsd:complexType>

    <!--
        *==============================================================================*
        *   Switch attributes                                                          *
        *==============================================================================*
    -->

    <xsd:complexType name="cobol_switch_Type">
        <xsd:attribute name="number" type="cobol_switch_NumberType" use="required"/>
        <xsd:attribute name="value"  type="cobol_switch_ValueType" use="required"/>
    </xsd:complexType>

    <xsd:complexType name="rts_switch_Type">
        <xsd:attribute name="name"  type="rts_switch_NameType" use="required"/>
        <xsd:attribute name="value" type="rts_switch_ValueType" use="required"/>
        <xsd:attribute name="index" type="rts_switch_IndexType" default="0"/>
    </xsd:complexType>

    <!--
        *==============================================================================*
        *   Switch attribute value restrictions                                        *
        *==============================================================================*
    -->

    <xsd:simpleType name="cobol_switch_NumberType">
        <xsd:restriction base="xsd:integer">
            <xsd:minInclusive value="0"/>
            <xsd:maxInclusive value="63"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="cobol_switch_ValueType">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="on"/>
            <xsd:enumeration value="off"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="rts_switch_NameType">
        <xsd:restriction base="xsd:string">
            <xsd:pattern value="[A-Z]"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="rts_switch_ValueType">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="on"/>
            <xsd:enumeration value="off"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="rts_switch_IndexType">
        <xsd:restriction base="xsd:integer">
            <xsd:minInclusive value="0"/>
            <xsd:maxInclusive value="7"/>
        </xsd:restriction>
    </xsd:simpleType>

